		escreva ( "Digite quarto numero:")
		leia (num4)

	    q1 = num1*num1
	    q2 = num2 * num2
	    q3 =num3*num3
	    q4= num4 * num4
	    se ( q3 >=1000)
	     escreva (q3)
	    //	q1 <=999 e q2 <=999 e q4 <=999)
	    
	     senao {
	    	escreva( "numeros: " , num1," "  , num2,  " " , num2, " ",  "quadrantes : " , q1, q2, q4)
	    	
	    }
	   
	}
}
