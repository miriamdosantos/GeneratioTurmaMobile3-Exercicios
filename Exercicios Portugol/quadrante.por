programa
{
	
	funcao inicio()
	{
		inteiro num1, num2, num3, num4
		inteiro q1,q2,q3,q4
		escreva ( "Digite um numero:")
		leia (num1)
		escreva ( "Digite segundo numero:")
		leia (num2)
		escreva ( "Digite terceiro numero:")
